{
   "action": {
      "default_popup": "html/popup-input.html"
   },
   "background": {
      "service_worker": "script/background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/index.css" ],
      "images": [  ],
      "js": [ "script/jquery-3.7.1.min.js", "script/content_script.js" ],
      "matches": [ "https://github.com/*" ],
      "run_at": "document_end"
   } ],
