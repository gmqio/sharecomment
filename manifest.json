      "128": "images/128.png",
      "16": "images/16.png",
      "256": "images/256.png",
      "32": "images/32.png",
      "64": "images/64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Github: Share Comment",
   "permissions": [ "contextMenus", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.6"
}
